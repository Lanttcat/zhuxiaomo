{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "ordered-imports": [
        false,
        {
          "import-sources-order": "lowercase-last",
          "named-imports-order": "lowercase-first"
        }
      ],
      "quotemark": [true, "single", "jsx-double"]
    },
    "rulesDirectory": []
}
